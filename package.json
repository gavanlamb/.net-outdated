{
  "name": "dotnet-outdated",
  "description": "Dotnet outdated GitHub Actions",
  "version": "0.0.0",
  "author": "Gavan Lamb",
  "homepage": "https://github.com/marketplace/actions/dotnet-outdated",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "bugs": {
    "url": "https://github.com/gavanlamb/dotnet-outdated/issues"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "dotnet",
    "outdated",
    "nuget",
    "package"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "npx eslint . -c .eslintrc.yml",
    "package": "npx ncc build src/index.ts -o dist --source-map --license licenses.txt",
    "test": "npx jest --json --outputFile=./test-results/summary.json",
    "all": "npm run lint && npm run test && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@octokit/auth-action": "^5.1.1",
    "@octokit/core": "^6.1.2",
    "@octokit/plugin-retry": "^7.1.1",
    "semver-diff": "^4.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@jest/types": "^29.6.3",
    "@types/node": "^20.12.12",
    "@types/jest-when": "^3.5.5",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jsonc": "^2.16.0",
    "jest": "^29.7.0",
    "jest-junit": "^13.0.0",
    "jest-when": "^3.6.0",
    "make-coverage-badge": "^1.2.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5"
  }
}
